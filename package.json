    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.2.0",
    "faiss-node": "0.5.1",
    "jsonwebtoken": "^9.0.0",
    "openai": "^4.72.0",
    "sequelize": "^6.37.5",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
